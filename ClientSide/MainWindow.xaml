<Window x:Class="ClientSide.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ClientSide"
        mc:Ignorable="d"
     Title="MainWindow" Height="300" Width="300">
    <StackPanel>
        <Image Height="200" Width="200" Source="{Binding ImageOpenClose}"></Image>
        <Button Height="50" Width="100" Margin="10" Command="{Binding StartCommand}">Start</Button>
    </StackPanel>
</Window>
